{
  "name": "runbikehike",
  "version": "0.2.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Run, Bike, Hike... allow you to record your running or biking sessions, so you can follow your sporting progress.",
  "main": "server.js",
  "scripts": {
    "check:js": "./scripts/check-js.sh",
    "build": "./scripts/build.sh",
    "dev": "NODE_ENV=dev nodemon --debug ./server.js",
    "start": "node server.js",
    "publish:cozy": "./scripts/publish-cozy.sh",
    "test": "mocha tests/tests.js",
    "cozy-test": "npm run deploy && cd publish/runbikehike-cozy && cozy-dev deploy 9550 && cd ../.. ",
    "deploy": "npm run build && npm run publish:cozy"
  },
  "config": {
    "ghooks": {
      "pre-commit": "./scripts/verify.sh"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/nicodel/runbikehike.git"
  },
  "author": "Nicolas Delebecque",
  "license": "MPL2.0 licence",
  "bugs": {
    "url": "https://gitlab.com/nicodel/runbikehike/issues"
  },
  "icon-path": "img/icon128.png",
  "dependencies": {
    "americano": "^0.4.5",
    "body-parser": "^1.15.2",
    "cozydb": "^0.1.13",
    "express": "^4.14.0",
    "pouchdb": "^6.0.5",
    "webl10n": "^1.0.0"
  },
  "devDependencies": {
    "csslint": "^1.0.2",
    "ghooks": "^1.3.2",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-express-server": "^0.5.3",
    "grunt-mocha": "^1.0.2",
    "grunt-mocha-test": "^0.13.2",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "should": "^11.1.0",
    "uglify-js": "^2.7.3",
    "uglifycss": "0.0.21"
  }
}
